<p id="notice"><%= notice %></p>

<h1>Pacientes</h1>

<table>
  <thead>
  <tr>
    <th>Id</th>
    <th>Nome</th>
    <th>Data nascimento</th>
    <th>Cpf</th>
    <th>Email</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @pacientes.each do |paciente| %>
    <tr>
      <td><%= paciente.id %></td>
      <td><%= paciente.nome %></td>
      <td><%= paciente.data_nascimento %></td>
      <td><%= paciente.cpf %></td>
      <td><%= paciente.email %></td>
      <td><%= link_to paciente.nome, paciente%></td>
    </tr>
  <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Paciente', new_paciente_path %>